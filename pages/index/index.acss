.container {
  padding: 10px;
}
.list-item {
  padding: 5px;
  width: 70px;
  height: 70px;
  text-align: center;
  font-size: 12px;
  margin: 10px;
}

.list-item image {
  box-sizing: border-box;
  width: 100%;
  height: 70px;
  padding: 12px;
  background-color: #fff;
  border: 1px solid #eee;
  border-radius: 4px;
  display: block;
  margin: 0 auto;
  margin-bottom: 10px;
}
